{
  "name": "YouTube Blocker",
  "description": "Block the tubes",
  "version": "2.0",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://youtube.com/*", "https://www.youtube.com/*"],
      "css": ["main.css"],
      "js": ["main.js"]
    }
  ],
  "manifest_version": 2
}
